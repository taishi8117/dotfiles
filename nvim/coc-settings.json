{
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSaveFiletypes": ["go"],
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "rust-analyzer.procMacro.enable": true,
  "codeLens.enable": false,
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.enableMessage": "jump",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": true,
  "diagnostic-languageserver.formatFiletypes": {
    "python": ["black", "isort", "docformatter"]
  },
  "diagnostic-languageserver.formatters": {
    "black": {
      "command": "black",
      "args": ["-q", "-"]
    },
    "isort": {
      "command": "isort",
      "args": ["-q", "-"]
    },
    "docformatter": {
      "command": "docformatter",
      "args": ["-"]
    }
  },
  "suggest.completionItemKindLabels": {
    "keyword": "\uf02d", //  book
    "variable": "\uf0e7", //  lightning bolt
    "value": "\uf51e", //  coins
    "operator": "\u002B", // + plus sign
    "constructor": "\uf662", //  toolbox
    "function": "\uf794", //  lambda
    "reference": "\uf0c1", //  link
    "constant": "\u03c0", // π greek letter pi
    "method": "\uf013", //  cog
    "struct": "\uf542", //  server tree
    "class": "\uf104", //  key
    "interface": "\uf1b3", //  cube
    "text": "\uf031", //  bold text
    "enum": "\ue096", //  layers
    "enumMember": "\uf477", //  posititve-space ellipsis
    "module": "\uf121", //  code
    "color": "\ue047", //  color palette
    "property": "\ufab7", // 﫷 rectangels with dots
    "field": "\uf15c", //  paperstack
    "unit": "\uf2dd", //  balance scale
    "event": "\uf133", //  calendar
    "file": "\uf0f6", //  filetext
    "folder": "\uf114", //  folder outline
    "snippet": "\uf5dc", //  brain
    "typeParameter": "\uf1c9", //  code branch
    "default": "\uf128" //  question chassis solid symbol
  },
  "snippets.textmateSnippetsRoots": ["~/.scissors-snippets"],
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.formatting.provider": "black"
}
